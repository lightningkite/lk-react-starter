{
  "name": "lk-react-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "npm run build:prod",
    "build:prod": "tsc --noEmit && vite build --mode production",
    "build:stage": "tsc --noEmit && vite build --mode staging",
    "serve": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@lightningkite/lightning-server-simplified": "^1.0.1",
    "@lightningkite/mui-lightning-components": "^2.2.1",
    "@mui/icons-material": "7.2.0",
    "@mui/lab": "^5.0.0-alpha.137",
    "@mui/material": "^7.2.0",
    "@mui/x-date-pickers": "^8.9.0",
    "@ngneat/falso": "^7.0.1",
    "dayjs": "^1.11.9",
    "eslint-import-resolver-typescript": "^3.5.5",
    "formik": "^2.4.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.14.2",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.19",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "tslib": "^2.6.0",
    "typescript": "^5.8.3",
    "vite": "^4.4.4",
    "vite-plugin-checker": "^0.6.1",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
