{
  "name": "lk-react-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "npm run build:prod",
    "build:prod": "tsc --noEmit && vite build --mode production",
    "build:stage": "tsc --noEmit && vite build --mode staging",
    "serve": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@lightningkite/lightning-server-simplified": "^0.4.1",
    "@lightningkite/mui-lightning-components": "^0.3.4",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.115",
    "@mui/material": "^5.11.4",
    "@mui/x-date-pickers": "^5.0.13",
    "@ngneat/falso": "^6.4.0",
    "dayjs": "^1.11.7",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@vitejs/plugin-react": "^3.0.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-checker": "^0.5.6",
    "vite-tsconfig-paths": "^4.0.3"
  }
}
